    <system:String x:Key="Automation.Type.PageProbenStd">Engine.Automation | Engine.Automation.Sparker.PageProbenStd</system:String>


    <!--会显示语法错误，但是可以运行-->
    <!--ItemsSource="{Binding Source={StaticResource Automation.Provider.MenuGeneric},XPath='/Root/FileSystem[@Name=\'ShellWindow\']/Folder'}"-->
    <system:String x:Key="Automation.XPath.PageInsElemBaseLib">Root/FileSystem[@Name='PageInsElemBaseLib']/Folder[@Enable='1']</system:String>
    <system:String x:Key="Automation.XPath.PageAnaElemBase">Root/FileSystem[@Name='PageAnaElemBase']/Folder[@Enable='1']</system:String>
    <system:String x:Key="Automation.XPath.PageRelatedSteel">Root/FileSystem[@Name='PageRelatedSteel']/Folder[@Enable='1']</system:String>


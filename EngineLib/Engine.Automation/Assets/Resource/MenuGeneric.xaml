    <system:String x:Key="Automation.Uri.PageInsElemBaseLib">pack://application:,,,/Engine.Automation;component/Engine.Automation.Sparker.Helper/Views/AnaPgm/PageInsElemBaseLib.xaml</system:String>
    
    <!--牌号管理相关页面-->
    <system:String x:Key="Automation.Uri.PageMaterial">pack://application:,,,/Engine.Automation;component/Engine.Automation.Sparker.Helper/Views/Material/PageMaterial.xaml</system:String>
    
    <!--控样管理相关页面-->
    <system:String x:Key="Automation.Uri.PageProben">pack://application:,,,/Engine.Automation;component/Engine.Automation.Sparker.Helper/Views/Proben/PageProben.xaml</system:String>
    <system:String x:Key="Automation.Uri.PageProbenStd">pack://application:,,,/Engine.Automation;component/Engine.Automation.Sparker.Helper/Views/Proben/PageProbenStd.xaml</system:String>
    

    <system:String x:Key="Automation.Type.PageAnaPgm">Engine.Automation | Engine.Automation.Sparker.PageAnaPgm</system:String>
